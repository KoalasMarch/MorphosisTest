<p id="notice"><%= notice %></p>

<h1>Product Sets</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Region</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_sets.each do |product_set| %>
      <tr>
        <td><%= product_set.product_id %></td>
        <td><%= product_set.region_id %></td>
        <td><%= link_to 'Show', product_set %></td>
        <td><%= link_to 'Edit', edit_product_set_path(product_set) %></td>
        <td><%= link_to 'Destroy', product_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Set', new_product_set_path %>
